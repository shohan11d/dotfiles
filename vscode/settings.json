      {
        "before": ["leader", "c", "b"],
        "commands": ["editor.action.blockComment"]
      },
  
      { "before": ["leader", "a"], "after": ["$"] },
      { "before": ["leader", "i"], "after": ["^"] },
  
      { "before": ["leader", "e"], "commands": ["editor.action.quickFix"] },
      { "before": ["leader", "l"], "commands": ["editor.action.marker.next"] },
      { "before": ["leader", "h"], "commands": ["editor.action.marker.prev"] },
  
      { "before": ["leader", "leader", "t"], "commands": [":tabonly"] },
      {
        "before": ["leader", "leader", "o"],
        "commands": ["workbench.action.closeEditorsInOtherGroups"]
      },
